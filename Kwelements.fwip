?MACRO HCON_JPKG <NME> ; <HS> ; <CS> ; <OS> ; <NS> ; <JPKG> ?
?HCON_MASS <NME> ; <HS> ; <CS> ; <OS> ; <NS> ?
CONST <NME>JPMOLE <JPKG> * Kw.JOULE / Kw.KILOGRAM
      * <NME>_MASS;  #ML2N-1T2
CONST <NME>JPKG <NME>JPMOLE / (<NME>_MASS); #L2T-2
CONST <NME>JPKGTOTAL <NME>JPMOLE / (<NME>_MASS
      + ((<HS>) / 4.0 + (<CS>) - (<OS>) / 2.0)
            * 2.0 * O_MASS); #L2T-2
?MACRO_END

?MACRO HCON_JPMOLE <NME> ; <HS> ; <CS> ; <OS> ; <NS> ; <JPMOLE> ?
?HCON_MASS  <NME> ; <HS> ; <CS> ; <OS> ; <NS> ?
